<%= simple_form_for [@pethome, @pet] do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
  <%= f.input :breed %>
  <%= f.input :weight %>
  <%= f.input :size %>
  <%= f.input :color %>
  <%= f.input :observations %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%# f.input :picture %>
  <%= f.submit "add pet", class: "btn btn-primary" %>
<% end %>
